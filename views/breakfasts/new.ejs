<% include ../partials/header %>

<h1>Create new Breakfast Dish to <%= restaurant.name %> </h1>

<form action="/restaurants/<%=restaurant._id%>/breakfasts" method="POST">
    <input type="text" name="breakfast[food]" placeholder="food">
    <input type="text" name="breakfast[price]" placeholder="price">
    <input type="submit"> 
    <a href="/restaurants">Go Back</a>
</form>

